msgid "%ssearch EPG"
msgstr "%sпошук ЕПГ"

msgid "%ssearch EPG from history..."
msgstr "%sпошук ЕПГ в історії..."

msgid "Add/remove/edit timer for highlighted event"
msgstr "Додати/видалити/редагувати таймер для виділеної події"

msgid "Add AutoTimer"
msgstr "Додати АвтоТаймер"

msgid "Add Timer"
msgstr "Додати Таймер"

msgid "Add a record timer for highlighted event"
